        function Master_CheckingMenuSelected() {
            let url = window.location.pathname;
            if ($('#BodyMain').hasClass('menutop-version')) {
                $("#TTmenu_Top .menudesktop_render").each((idx, ele) => {
                    let currlink = $(ele).attr("href");
                    if (currlink.includes(url)) {
                        if (ele.id != "menuCustomerCreate") {
                            MS_UrlRoot = "";
                            MS_UrlChild = $(ele).text();
                            Master_SetTitle();
                        }
                    }

                });
            }
            else {
                $("#menuDesktopCategory .menudesktop_render").each((idx, ele) => {
                    let currlink = $(ele).attr("href");
                    $(ele).removeClass("active");
                    if (currlink.includes(url)) {
                        if (ele.id != "menuCustomerCreate") {
                            $(ele).addClass("active");
                            let collapseItem = $(ele).closest(".collapsesticky");
                            let collapseItemContent = collapseItem.parent().siblings('.nav-item');
                            collapseItem.siblings('.nav-link').addClass("active");
                            collapseItem.addClass("show");
                            collapseItemContent.children('.menuparent_render').removeClass('active');
                            collapseItemContent.children('.collapsesticky').removeClass('show');
                            MS_UrlRoot = collapseItem.siblings('.nav-link').find(".nav-link-text").text();
                            MS_UrlChild = $(ele).children('.sidenav-normal').text();
                            Master_SetTitle();
                        }
                    }
                });
            }


        }
        function Master_SetTitle () {
            try {
                if (MS_UrlRoot != "" || MS_UrlChild != "") {
                    document.title = MS_UrlRoot != "" ? (MS_UrlRoot + ' - ' + MS_UrlChild) : MS_UrlChild;
                }
               else  document.title = 'VTTECH SOLUTION';
            }
            catch (ex) {
                document.title = 'VTTECH SOLUTION';
            }
        }
        function Master_Evenmenutop () {
            $('#TTmenu_Top .nav-link').unbind('click').click(function (e) {
                e.preventDefault();
                if ($(this).hasClass('active')) {
                    $('#TTmenu_Top .nav-link').removeClass("active").removeClass("show");
                    $('#TTmenu_Top .dropdown-menu').removeClass("show");
                }
                else {
                    $('#TTmenu_Top .nav-link').removeClass("active").removeClass("show");
                    $('#TTmenu_Top .dropdown-menu').removeClass("show");

                    $(this).addClass("active").addClass("show");
                    $(this).siblings(".dropdown-menu").addClass("show");
                }


            })
        }
        function Master_EventRedirect() {
            $('#menuDesktopCategory').on('click', '.menuparent_render', function (e) {
                $('#menuDesktopCategory .menuparent_render').removeClass('active');
                $(this).closest(".collapsesticky").siblings(".menuparent_render").addClass("active");
                $(this).addClass("active");
            })
            $('#menuDesktopCategory').on('click', '.menudesktop_render', function (e) {
                e.preventDefault();
                $('#menuDesktopCategory .menudesktop_render').removeClass('active');
                let namechild = '', namegroup = '';
                namechild = $(this).attr("data-name");
                $(this).addClass('active');
                let objparent = $(this).parent().parent();
                if (objparent != undefined) {
                    objparent.addClass('active');
                    namegroup = objparent.attr("data-name");
                }
                let href = $(this).attr('href');
                MS_UrlRoot = namegroup;
                MS_UrlChild = namechild;
                window.history.pushState(href, 'Title', href);
                Master_RedirectLoadPage(href);
                if(sys_isMobile==1) $('#BodyMain').removeClass('g-sidenav-pinned').addClass('g-sidenav-hidden');
            });
            $('#TTmenu_Top').on('click', '.menudesktop_render', function (e) {
                e.preventDefault();
                let namechild = '', namegroup = '';
                namechild = $(this).attr("data-name");
                let objparent = $(this).parent().parent();
                if (objparent != undefined) {
                    namegroup = objparent.attr("data-name");
                }
                let href = $(this).attr('href');
                MS_UrlRoot = namegroup;
                MS_UrlChild = namechild;
                window.history.pushState(href, 'Title', href);
                Master_RedirectLoadPage(href);
            });
            window.addEventListener('popstate', function (e) {
                if (e.state) Master_RedirectLoadPage(e.state);
            });
        }

