        function Hard_Reload_LoadSessionBase(datas) {
            try {
                var promises = [];
                let i = 1;
                for (const [key, value] of Object.entries(datas)) {
                    promises.push(Master_IndexDB_IniTable(Master_DBName, Master_TableName[i - 1], value, i));
                    i++;
                }
                Promise.all(promises).then((values) => {
                    window.location.reload(true)
                });
            }
            catch (ex) {
                window.location.reload(true);
            }
        }


