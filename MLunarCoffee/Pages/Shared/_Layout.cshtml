        async function MS_RefernceIP () {
            new Promise((reolve) => {
                setTimeout(() => {
                    try {
                        let _branchname = '';
                        let _notice = false;
                        let _ip = author_get('ip');
                        if (_ip != undefined && _ip.length>10) {
                            Master_IndexDB_Reads(_Session_Data, [_Session_Branch], function (data) {
                                let branch = data[0];
                                let _branchcur = branch[Master_branchID];
                                if (_branchcur != undefined && _branchcur!="") {
                                    let _rangeip = _branchcur.IP;
                                    _branchname = _branchcur.Name;
                                    if (_rangeip != undefined && _rangeip.length > 10) {
                                        _ip = ';' + _ip + ';'
                                        _rangeip = ';' + _rangeip + ';';
                                        if (!_rangeip.includes(_ip)) _notice = true;
                                    }

                                }
                                if (_notice) {
                                    notiWarning('@Local["Chi nhánh phần mềm"] ' + _branchname);
                                }
                            });
                        }

                    }
                    catch (ex) {
                    }
                }, 1000)
            })
        }
