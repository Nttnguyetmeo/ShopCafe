@{
    @using  MLunarCoffee.Comon;
    @using  MLunarCoffee.Comon.GlobalStore;
    @using  MLunarCoffee.Comon.Firebase

    Layout = "_Layout";
    ViewData["LINK_SOURCE"] = "";
    ViewData["VERSION"] = Global.sys_DBVersion;
    ViewData["TIME"] = DateTime.Now.Ticks;
    ViewData["NAME"] = Global.sys_NameClient;
    ViewData["CODE"] = Global.APICODE;
    ViewData["PrimaryColor"] = Global.sys_DefaultColor;
    ViewData["PermissionTele"] = Global.sys_Permission_Tele;
    ViewData["SessionData"] = SessionName.Session_Data;
    ViewData["Session_Branch"] = SessionName.Seesion_Branch;
    ViewData["Session_Teeth"] = SessionName.Seesion_Teeth;
    ViewData["Session_Service"] = SessionName.Seesion_Service;
    ViewData["Session_ServiceCare"] = SessionName.Seesion_ServiceCare;
    ViewData["Session_Employee"] = SessionName.Seesion_Employee;
    ViewData["Session_User"] = SessionName.Session_User;
    ViewData["Session_City"] = SessionName.Session_City;
    ViewData["Session_District"] = SessionName.Session_District;
    ViewData["Session_National"] = SessionName.Session_National;
    ViewData["Session_Commune"] = SessionName.Session_Commune;

    #region // Firebase message
    ViewData["apiKey"] = FirebaseInfo.FBapiKey;
    ViewData["authDomain"] = FirebaseInfo.FBauthDomain;
    ViewData["projectId"] = FirebaseInfo.FBprojectId;
    ViewData["storageBucket"] = FirebaseInfo.FBstorageBucket;
    ViewData["messagingSenderId"] = FirebaseInfo.FBmessagingSenderId;
    ViewData["appId"] = FirebaseInfo.FBappId;
    ViewData["databaseURL"] = FirebaseInfo.FBdatabaseURL;
    ViewData["measurementId"] = FirebaseInfo.FBmeasurementId;

    #endregion

}

