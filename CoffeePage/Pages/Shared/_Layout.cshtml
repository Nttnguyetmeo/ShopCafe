        author_set("UserLang", 'vi');
        const Outlang = Master_Load_Lan();
        function Master_Load_Lan() {
            let _lan = author_get("UserLang");
            try {
                switch (_lan) {
                    case 'vi':
                        js_require_notasync('/Language/OutResource.vi.js', true);
                        break;
                    case 'en':
                        js_require_notasync('/Language/OutResource.en.js', true);
                        break;
                    default: break;
                }
                if (typeof DataOutlang !== 'object') return {};
                return DataOutlang.reduce((pre, arr) => {
                    if (arr.Key) pre[arr.Key] = arr.Value;
                    return pre;
                }, {})
            }
            catch (e) {
                return {};
            }
        }
